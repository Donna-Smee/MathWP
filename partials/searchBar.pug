style
    include ../public/searchBar.css

div#search
    input#searchInput(type="text" placeholder="Search for work sheets here")
    select#sectionSelect(name="section") 
        option(value="") Select a section
        option(value="number") Number
        option(value="algebra") Algebra 
        option(value="data") Data 
        option(value="spacial") Spacial Sense    
        option(value="finance") Financial Literacy  
    
    select#gradeSelect(name="section") 
        option(value="") Select a grade
        option(value=1) 1
        option(value=2) 2 
        option(value=3) 3 
        option(value=4) 4    
        option(value=5) 5
        option(value=6) 6 
        
    button#searchBut(onclick="searchWS()") Search